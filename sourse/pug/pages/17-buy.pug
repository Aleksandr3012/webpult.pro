extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug
include ../blocks/sBlog/_sBlog.pug
include ../blocks/sPossibilities/_sPossibilities.pug
include ../blocks/sHave/_sHave.pug
include ../blocks/sPreview/_sPreview.pug
include ../blocks/sSupportTable/_sSupportTable.pug
include ../blocks/sHowOrder/_sHowOrder.pug
include ../blocks/sLicenses/_sLicenses.pug

block variables
	- let  mainPage= null
block title
	- let  title= 'Лицензии 1С-Битрикс управление сайтом'
	title= title
block pagecontent
	+header()
		+topLine()
	.breadcrumbWrap
		.container
			+breadcrumb('Главная', 'Лицензии', title)
		+headerBlockBody().--buy
			.container
				+b.section-title
					h1 Купить «1С-Битрикc: Управление сайтом»

	+sPreview('bus.png',`&laquo;1С-Битрикс: Управление сайтом&raquo;&nbsp;&mdash; профессиональная система управления веб-проектами`)(class="sPreview--buy")
		p &laquo;1С-Битрикс: Управление сайтом&raquo;&nbsp;&mdash; профессиональная система управления веб-проектами, универсальный программный продукт для создания, поддержки и&nbsp;успешного развития сайтов и&nbsp;интернет-магазинов любого уровня.
		p Сайты на&nbsp;платформе 1С-Битрикс&nbsp;&mdash; это удобство, надежность и&nbsp;высокая посещаемость.
	+sLicenses()

	+sHave17
	+footer()


	