mixin sPossibilities(data)
	// start sPossibilities
	+b.sPossibilities.section#sPossibilities&attributes(attributes)
		.container
			.tabs
				+e.caption.tabs__caption
					+e.btn-tab.tabs__btn.active
						+icon("cloud")
						| Облачное решение
					+e.btn-tab.tabs__btn
						+icon("server")
						| Коробочное решение
				div
					.tabs__content.active
						//- .active
						+e.TABLE.table--cloud
							thead
								tr
									th
										+e.price-text Цена за месяц при  покупке:
										+input("на 2 года",'select')
											//- option на 2 года
											//- option на 2 года
											//- option на 2 года
									mixin item(t,p,op, np,free)
										th
											+e.item
												+e.title!= t
												+e.text!= p
												+e.price--old
													if op
														span= op
														| рублей
												if free
													+e.price--new.text-success Бесплатно
												else
													+e.price--new= np
												
												+e.economy
													if op
														span Экономия -33% 
												+e.footer
													if free
														+e.btn.btn-success(href="#") Создать

													else
														+e.btn.btn-primary(href="#") Купить
												.d-xl-none.pt-3
													+e.more.link-modal.tdu(href="#") Подробнее
									+item("Бесплатный",`Необходимый <br> минимум <br> инструментов для начала работы`,'','','free')
									+item("Старт+",`Сайты и CRM - удачное начало вашего<br> бизнеса`,'','990 рублей/мес.')
									+item("CRM+",'Идеально для небольших отделов продаж','','2 990 рублей/мес.')
									+item("Задачи+",'Оптимальное решение для работы над проектами, контроля сроков и качества','1 499 900  ','990 рублей/мес.')
									+item("Команда",'Лучший выбор для автоматизации продаж в CRM и проектной работы','1 499 900 ','5 990 рублей/мес.')
									+item("Компания",'Максимальные возможности для всей компании','1 499 900   ','11 990 рублей/мес.')
							tbody
								mixin tr(data)
									tr
										td
											if data.t0
												= data.t0 
											else
												block
										td
											if data.t1
												.small= data.t1
											else if data.plus1
												+icon('minus')
											else
													+icon('plus')
												
										td
											if data.t2
												.small= data.t2
											else if data.plus2
												+icon('minus')
											else
												+icon('plus')
												
